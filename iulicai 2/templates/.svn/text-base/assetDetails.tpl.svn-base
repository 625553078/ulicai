<div id="assetDetails-wrap">
    <ul id="assetDetails">
        <li class="cur" data-type="all">全部</li>
        <li data-type="undue">未到期</li>
        <li data-type="repay">还款中</li>
        <li data-type="paid">已还款</li>
    </ul>

    <div class="assetList">
        <article class="all">
            {{#all}}
            <div>
                <ul>
                    <li>标的名称 <span >{{title}}</span></li>
                    <li>投资金额 <span>{{sum}}</span></li>
                    <li>投资时间 <span>{{time}} 剩余{{leftDays}}天</span></li>
                    <li>累计收益 <span>{{income}}</span></li>
                    <li>标的状态 <span>{{status}}</span></li>
                </ul>
                <p class="desc">期限{{term}}天，收益率{{rate}}</p>
            </div>
            {{/all}}
        </article>

        <article class="undue">
            {{#undue}}
            <div>
                <ul>
                    <li>标的名称 <span >{{title}}</span></li>
                    <li>投资金额 <span>{{sum}}</span></li>
                    <li>投资时间 <span>{{time}} 剩余{{leftDays}}天</span></li>
                    <li>累计收益 <span>{{income}}</span></li>
                    <li>标的状态 <span>{{status}}</span></li>
                </ul>
                <p class="desc">期限{{term}}天，收益率{{rate}}</p>
            </div>
            {{/undue}}
        </article>

        <article class="repay">
        {{#repay}}
        <div>
            <ul>
                <li>标的名称 <span >{{title}}</span></li>
                <li>投资金额 <span>{{sum}}</span></li>
                <li>投资时间 <span>{{time}} 剩余{{leftDays}}天</span></li>
                <li>累计收益 <span>{{income}}</span></li>
                <li>标的状态 <span>{{status}}</span></li>
            </ul>
            <p class="desc">期限{{term}}天，收益率{{rate}}</p>
        </div>
        {{/repay}}
        </article>

        <article class="paid">
        {{#paid}}
            <div>
                <ul>
                    <li>标的名称 <span >{{title}}</span></li>
                    <li>投资金额 <span>{{sum}}</span></li>
                    <li>投资时间 <span>{{time}} 剩余{{leftDays}}天</span></li>
                    <li>累计收益 <span>{{income}}</span></li>
                    <li>标的状态 <span>{{status}}</span></li>
                </ul>
                <p class="desc">期限{{term}}天，收益率{{rate}}</p>
            </div>
        {{/paid}}
        </article>
    </div>
</div>