<div id="reco-wrap">
    <div id="defaultView">

    		<h3>{{productName}}</h3>
    		<div class="reco-main">
    			<span class="topRate">{{usedRate}}54%</span>
    			<div class="circle-out">
    				 <div class="circle-wrap">
    					<div class="deadline">预期年化收益</div>
    					<div class="effects" data-effects="{{rate}}">{{rate}}<span data="加送{{extRate}}%">%</span></div>
    					<div class="expect" data-expect="{{prodPeriod}}">理财期限</div>
    				 </div>
    			 </div>
    		</div>
    		<div class="desc">
    			<ul>
    				<li data="当日起息">计息方式</li>
    				<li data="{{restAmount}}">剩余可投（元）</li>
    				<li data="{{totalAmount}}">总金额（万）</li>
    			</ul>

    			<!--a class="buy" href="/ulicai/index.html?#/productDetails?productId={{productId}}">{{minBuyAmount}}元起投</a-->
    			<a class="buy" href="#">{{minBuyAmount}}元起投</a>
    			<p class="safe-info">个人账户资产由阳光保险100%承保</p>
    		</div>
    </div>
</div>

